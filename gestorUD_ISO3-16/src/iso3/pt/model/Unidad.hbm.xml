<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="iso3.pt.model.Unidad" table="Unidad">

        <id name="id" column="UD_ID">
            <generator class="native"/>
        </id>
        
        <property name="acronimo" column="UD_ACRON" unique="true" not-null="true"/>
		<property name="titulo" column="UD_TITULO" not-null="true"/>
        <property name="contenido" column="UD_CONTENIDO"/>

    </class>

</hibernate-mapping>
